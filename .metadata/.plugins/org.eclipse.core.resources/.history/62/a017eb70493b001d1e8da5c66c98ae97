package es.florida.tema1;

import java.util.Scanner;

public class AP12 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		int[] notas = new int[2];
		int[] suspensos = new int[2];
		int[] aprobados = new int[2];
		int[] notables = new int[2];
		int[] sobresalientes = new int[2];

		for (int i = 0; i < notas.length; i++) {
			int anum = i+1;
			System.out.print("Añada la nota de alumno"+"#"+anum+": ");
			notas[i] = sc.nextInt();
		}
		for (int i = 0; i < notas.length; i++) {
			if (notas[i] >= 9) {
				sobresalientes[i] = notas[i];
			}else if (notas[i] >= 7 ) {
				notables[i] = notas[i];
			}else if (notas[i] >= 5 ) {
				aprobados[i] = notas[i];
			}else{
				suspensos[i] = notas[i];
			}
		}
		System.out.println("Notas: ");
		System.out.println("Suspendidos: "+suspensos.toString());
		System.out.println("Aprobados: "+aprobados.toString());
		System.out.println("Notables: "+notables.toString());
		System.out.println("Sobresalientes: "+sobresalientes.toString());

	}

}
