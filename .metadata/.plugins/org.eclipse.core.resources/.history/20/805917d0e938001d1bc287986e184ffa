package es.florida.tema1;

import java.util.Scanner;

public class AP5 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		int num1;
		int num2;
		
		do {
			System.out.print("Numero 1:");
			num1 = sc.nextInt();
			System.out.print("Numero 2:");
			num2 = sc.nextInt();
		}while(num1!=num2);
		
		
		
		
	}

}
